--><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
   <modelVersion>4.0.0</modelVersion>

   <organization>
      <name>Genesys Telecommunication Laboratories, Inc.</name>
      <url>http://www.genesys.com</url>
   </organization>
   <name>PSDK :: Universal Contact Server Protocol</name>

   <parent>
      <groupId>com.genesyslab.platform</groupId>
      <artifactId>protocols-bom</artifactId>
      <version>900.10.28</version>
   </parent>
   <artifactId>contactsprotocol</artifactId>
   <packaging>jar</packaging>

   <properties>
      <automatic.module.name>com.genesyslab.platform.contactsprotocol</automatic.module.name>

      <protocol.jar.name>${project.build.finalName}</protocol.jar.name>

      <!-- Empty by default. may be overridden in osgi profile -->
      <manifest.file/>
      <manifest.implementation.title>com.genesyslab.platform.contacts.protocol</manifest.implementation.title>
      <manifest.component.path>com/genesyslab/platform/contacts/protocol</manifest.component.path>

      <basexml.dir>${basedir}/../../psdk_xml/ContactServerProtocolsXml/Src</basexml.dir>
      <doc.dir>${basedir}/../../psdk_doc/projects/protocols/ContactServerProtocolsJava</doc.dir>
      <doc.dir.xml>${basedir}/../../psdk_doc/projects/protocols/ContactServerProtocolsXml/Src</doc.dir.xml>
      <sdk_commons.dir>${basedir}/../../sdk_commons</sdk_commons.dir>

      <!-- binding generator params -->
      <generator.binding.main>com.genesyslab.platform.commons.kwgenerator.Generator</generator.binding.main>
      <generator.binding.xml>ContactServer.xml</generator.binding.xml>
      <generator.binding.output>${project.basedir}/src/generated</generator.binding.output>
      <generator.binding_old.xsl>${basedir}/src/main/kvbinding/xsl/kvgenerator_java_old.xsl</generator.binding_old.xsl>
      <generator.binding.xsl>${basedir}/src/main/kvbinding/xsl/kvgenerator_java_new.xsl</generator.binding.xsl>
      <generator.binding.comments>${basedir}/../../psdk_doc/projects/protocols/ContactServerProtocolsXml/Src/ContactServerComments.xml</generator.binding.comments>
      <kvbinding.dir>${basedir}/tools/kvbinding</kvbinding.dir>
      <kvbinding.desc>ContactServerNew.xml</kvbinding.desc>
      <customtypes.xsd>CustomTypes.xsd</customtypes.xsd>
      <kvbinding.out.file>${basedir}/src/generated/com/genesyslab/platform/contacts/protocol/contactserver/runtime/ContactServerBinding.java</kvbinding.out.file>
      <kvbinding.out.file.test>${basedir}/src/test/generated/com/genesyslab/platform/contacts/protocol/contactserver/runtime/ContactServerBindingCompatibilityTests.java</kvbinding.out.file.test>
      <kvbinding.out.file.ha.test>${basedir}/src/test/generated/com/genesyslab/platform/contacts/protocol/contactserver/TestHiddenAttributes.java</kvbinding.out.file.ha.test>
      <javatools.dir>${basedir}/../JavaTools/KVGenerator</javatools.dir>

      <!-- contacts generator params -->
      <generator.contacts.main>com.genesyslab.platform.commons.protocol.generator.Generator</generator.contacts.main>
      <generator.contacts.xml>ContactServer.xml</generator.contacts.xml>
      <generator.contacts.comments>${basedir}/../../psdk_doc/projects/protocols/ContactServerProtocolsXml/Src/ContactServerComments.xml</generator.contacts.comments>
      <generator.contacts.output>${project.basedir}/src/generated</generator.contacts.output>

      <!-- xslt params -->
      <xslt.work.dir>${project.basedir}/tools/MessageGenerator</xslt.work.dir>
      <xml.dir>${project.basedir}/tools/MessageGenerator</xml.dir>

      <classes.for.unit.test>${project.build.outputDirectory}</classes.for.unit.test>

      <!-- Plugin versions -->
      <version.emma.plugin>1.2</version.emma.plugin>
      <version.emma4it.plugin>1.3</version.emma4it.plugin>
      <version.dependency.plugin>2.2</version.dependency.plugin>
      <version.resource.plugin>2.5</version.resource.plugin>
      <version.properties.plugin>1.0-alpha-2</version.properties.plugin>
      <version.assembly.plugin>2.2.1</version.assembly.plugin>
      <version.ant.plugin>1.3</version.ant.plugin>
      <version.cobertura.plugin>2.7</version.cobertura.plugin>
      <version.findbugs.plugin>2.5.2</version.findbugs.plugin>
      <version.info-reports.plugin>2.4</version.info-reports.plugin>
      <version.xml.maven.plugin>1.0</version.xml.maven.plugin>

      <version.saxon>8.8</version.saxon>
      <version.saxon-dom>8.8</version.saxon-dom>
      <version.ant-trax>1.7.1</version.ant-trax>
   </properties>

   <dependencies>
      <dependency>
         <groupId>com.genesyslab.platform</groupId>
         <artifactId>openmediaprotocol</artifactId>
      </dependency>
      <dependency>
         <groupId>com.genesyslab.platform</groupId>
         <artifactId>generator</artifactId>
         <type>jar</type>
         <scope>runtime</scope>
         <optional>true</optional>
      </dependency>
      <dependency>
         <groupId>com.genesyslab.platform</groupId>
         <artifactId>kvbindinggenerator</artifactId>
         <scope>runtime</scope>
         <optional>true</optional>
      </dependency>
      <dependency>
         <groupId>net.sf.saxon</groupId>
         <artifactId>saxon</artifactId>
         <version>${version.saxon}</version>
         <scope>runtime</scope>
         <optional>true</optional>
      </dependency>
      <dependency>
         <groupId>net.sf.saxon</groupId>
         <artifactId>saxon-dom</artifactId>
         <version>${version.saxon}</version>
         <scope>runtime</scope>
         <optional>true</optional>
      </dependency>
      <dependency>
         <groupId>org.apache.ant</groupId>
         <artifactId>ant-trax</artifactId>
         <version>${version.ant-trax}</version>
         <scope>runtime</scope>
         <optional>true</optional>
      </dependency>

      <dependency>
         <groupId>javax.annotation</groupId>
         <artifactId>javax.annotation-api</artifactId>
      </dependency>

      <dependency>
         <groupId>org.testng</groupId>
         <artifactId>testng</artifactId>
         <version>${version.testng}</version>
         <scope>test</scope>
      </dependency>
      <dependency>
         <groupId>junit</groupId>
         <artifactId>junit</artifactId>
         <version>${version.junit}</version>
         <scope>test</scope>
      </dependency>
   </dependencies>

   <build>
      <finalName>${project.artifactId}</finalName>
      <plugins>
         <!-- Generates XSD schema, prepares contacts protocol classes generation -->
         <plugin>
            <artifactId>maven-clean-plugin</artifactId>
            <configuration>
               <filesets>
                  <fileset>
                     <directory>${generator.binding.output}</directory>
                     <includes>
                        <include>**/*.*</include>
                     </includes>
                  </fileset>
               </filesets>
            </configuration>
         </plugin>
         <plugin>
            <groupId>org.codehaus.mojo</groupId>
            <artifactId>xml-maven-plugin</artifactId>
            <version>${version.xml.maven.plugin}</version>
            <executions>
               <execution>
                  <id>generate-sources</id>
                  <phase>generate-sources</phase>
                  <goals>
                     <goal>transform</goal>
                  </goals>
               </execution>
            </executions>
            <configuration>
               <transformationSets>
                  <transformationSet>
                     <dir>${basexml.dir}</dir>
                     <stylesheet>${generator.binding.xsl}</stylesheet>
                     <includes>
                        <inclide>${kvbinding.desc}</inclide>
                     </includes>
                     <outputDir>${generator.binding.output}</outputDir>
                     <parameters>
                        <parameter>
                           <name>rootFolder</name>
                           <value>${generator.binding.output}</value>
                        </parameter>
                        <parameter>
                           <name>baseTypesFile</name>
                           <value>../../../../../JavaTools/KVGenerator/base_types.xml</value>
                        </parameter>
                        <parameter>
                           <name>customTypesFile</name>
                           <value>../../../../tools/kvbinding/custom_types.xml</value>
                        </parameter>
                        <parameter>
                           <name>commentsFile</name>
                           <value>../../../../../../psdk_doc/projects/protocols/ContactServerProtocolsXml/Src/ContactServerComments.xml</value>
                        </parameter>
                     </parameters>
                  </transformationSet>
               </transformationSets>
            </configuration>
            <dependencies>
               <dependency>
                  <groupId>net.sf.saxon</groupId>
                  <artifactId>saxon</artifactId>
                  <version>8.7</version>
               </dependency>
            </dependencies>
         </plugin>
         <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-antrun-plugin</artifactId>
            <version>${version.ant.plugin}</version>
            <executions>
               <execution>
                  <phase>initialize</phase>
                  <id>generate-contactservermessages-xsd</id>
                  <configuration>
                     <tasks>
                        <copy file="${basexml.dir}\${customtypes.xsd}" tofile="${xslt.work.dir}\${customtypes.xsd}"/>
                        <echo>Generating ContactServerMessages.xsd...</echo>
                        <xslt basedir="${xslt.work.dir}" destdir="${xslt.work.dir}" in="${basexml.dir}\ContactServerNew.xml" out="${xslt.work.dir}\ContactServerMessages.xsd" style="${xslt.work.dir}\XSDmessageNew.xsl"/>
                        <delete file="${xslt.work.dir}\${customtypes.xsd}"/>
                     </tasks>
                  </configuration>
                  <goals>
                     <goal>run</goal>
                  </goals>
               </execution>
               <execution>
                  <id>generate-kvbinding-sources</id>
                  <phase>generate-sources</phase>
                  <configuration>
                     <tasks>
                        <copy file="${basexml.dir}\${kvbinding.desc}" tofile="${kvbinding.dir}\${kvbinding.desc}"/>
                        <copy file="${javatools.dir}\kvbinding.xsl" tofile="${kvbinding.dir}\kvbinding.xsl"/>
                        <copy file="${javatools.dir}\templates.xsl" tofile="${kvbinding.dir}\templates.xsl"/>
                        <copy file="${javatools.dir}\base_types.xml" tofile="${kvbinding.dir}\base_types.xml"/>
                        <copy file="${javatools.dir}\tests.xsl" tofile="${kvbinding.dir}\tests.xsl"/>
                        <delete file="${kvbinding.out.file}"/>
                        <xslt basedir="${kvbinding.dir}" destdir="${kvbinding.dir}" in="${kvbinding.dir}\${kvbinding.desc}" out="${kvbinding.out.file}" style="${kvbinding.dir}\kvbinding.xsl">
                           <param expression=".runtime" name="package_postfix"/>
                        </xslt>
                        <delete file="${kvbinding.out.file.ha.test}"/>
                        <xslt basedir="${kvbinding.dir}" destdir="${kvbinding.dir}" in="${kvbinding.dir}\${kvbinding.desc}" out="${kvbinding.out.file.ha.test}" style="${kvbinding.dir}\hidden_attributes_tests.xsl">
                           <param expression=".runtime" name="package_postfix"/>
                        </xslt>
                        <delete file="${kvbinding.dir}\${kvbinding.desc}"/>
                        <delete file="${kvbinding.dir}\${kvbinding.desc}"/>
                        <delete file="${kvbinding.dir}\kvbinding.xsl"/>
                        <delete file="${kvbinding.dir}\templates.xsl"/>
                        <delete file="${kvbinding.dir}\base_types.xml"/>
                        <delete file="${kvbinding.dir}\tests.xml"/>
                     </tasks>
                  </configuration>
                  <goals>
                     <goal>run</goal>
                  </goals>
               </execution>
            </executions>
            <dependencies>
               <dependency>
                  <groupId>org.apache.ant</groupId>
                  <artifactId>ant-trax</artifactId>
                  <version>${version.ant-trax}</version>
                  <scope>runtime</scope>
                  <optional>true</optional>
               </dependency>
               <dependency>
                  <groupId>net.sf.saxon</groupId>
                  <artifactId>saxon</artifactId>
                  <version>${version.saxon}</version>
                  <scope>runtime</scope>
                  <optional>true</optional>
               </dependency>
            </dependencies>
         </plugin>
         <plugin>
            <groupId>org.codehaus.mojo</groupId>
            <artifactId>exec-maven-plugin</artifactId>
            <executions>
               <execution>
                  <id>generator-kvbinding</id>
                  <phase>generate-sources</phase>
                  <goals>
                     <goal>exec</goal>
                  </goals>
                  <configuration>
                     <executable>java</executable>
                     <arguments>
                        <argument>-classpath</argument>
                        <classpath>
                           <dependency>com.genesyslab.platform:kvbindinggenerator</dependency>
                           <dependency>net.sf.saxon:saxon</dependency>
                           <dependency>net.sf.saxon:saxon-dom</dependency>
                        </classpath>
                        <argument>${generator.binding.main}</argument>
                        <argument>-xml</argument>
                        <argument>${generator.binding.xml}</argument>
                        <argument>-out</argument>
                        <argument>${generator.binding.output}</argument>
                        <argument>-xsl</argument>
                        <argument>${generator.binding_old.xsl}</argument>
                        <argument>-doc</argument>
                        <argument>${generator.binding.comments}</argument>
                     </arguments>
                     <workingDirectory>${basexml.dir}</workingDirectory>
                  </configuration>
               </execution>
               <execution>
                  <id>generator-contacts</id>
                  <phase>generate-sources</phase>
                  <goals>
                     <goal>exec</goal>
                  </goals>
                  <configuration>
                     <executable>java</executable>
                     <arguments>
                        <argument>-classpath</argument>
                        <classpath>
                           <dependency>com.genesyslab.platform:generator</dependency>
                           <dependency>net.sf.saxon:saxon</dependency>
                        </classpath>
                        <argument>${generator.contacts.main}</argument>
                        <argument>${generator.contacts.xml}</argument>
                        <argument>${generator.contacts.output}</argument>
                        <argument>${generator.contacts.comments}</argument>
                     </arguments>
                     <workingDirectory>${basexml.dir}</workingDirectory>
                  </configuration>
               </execution>
            </executions>
         </plugin>

         <!-- Add generated protocol classes to a build path -->
         <plugin>
            <groupId>org.codehaus.mojo</groupId>
            <artifactId>build-helper-maven-plugin</artifactId>
            <executions>
               <execution>
                  <phase>process-sources</phase>
                  <goals>
                     <goal>add-source</goal>
                  </goals>
                  <configuration>
                     <sources>
                        <source>${basedir}/src/generated</source>
                     </sources>
                  </configuration>
               </execution>
               <execution>
                  <id>add-test-source</id>
                  <phase>generate-test-sources</phase>
                  <goals>
                     <goal>add-test-source</goal>
                   </goals>
                   <configuration>
                      <sources>
                         <source>${basedir}/src/test/generated</source>              
                      </sources>
                   </configuration>
                </execution>
            </executions>
         </plugin>

         <!-- compile protocol -->
         <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-compiler-plugin</artifactId>
         </plugin>

         <!-- test protocol -->
         <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-surefire-plugin</artifactId>
            <configuration>
               <classesDirectory>${classes.for.unit.test}</classesDirectory>
               <classpathDependencyExcludes>
                  <classpathDependencyExcludes>net.sf.saxon:saxon</classpathDependencyExcludes>
               </classpathDependencyExcludes>
               <suiteXmlFiles>
                  <suiteXmlFile>tools\testng.xml</suiteXmlFile>
               </suiteXmlFiles>
               <additionalClasspathElements>
                  <additionalClasspathElement>bc-fips-1.0.2.1.jar</additionalClasspathElement>
<!--                  <additionalClasspathElement>bctls-fips-1.0.6.jar</additionalClasspathElement>-->
               </additionalClasspathElements>
            </configuration>
         </plugin>

         <!-- create jar -->
         <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-jar-plugin</artifactId>
            <executions>
               <execution>
                  <id>default-jar</id>
                  <configuration>
                     <archive>
                        <manifestFile>${manifest.file}</manifestFile>
                        <manifestEntries>
                           <Implementation-Title>${manifest.implementation.title}</Implementation-Title>
                           <Implementation-Version>${GENESYS.PROJECT.VERSION}</Implementation-Version>
                           <Implementation-Vendor>${project.organization.name}</Implementation-Vendor>
                           <Specification-Title>${project.name}</Specification-Title>
                           <Specification-Version>${GENESYS.PROJECT.VERSION}</Specification-Version>
                           <Specification-Vendor>${project.organization.name}</Specification-Vendor>
                           <Comments>${BUILDTYPE}-${env.COMPUTERNAME}-${maven.build.timestamp}</Comments>
                        </manifestEntries>
                        <manifestSections>
                           <manifestSection>
                              <name>${manifest.component.path}</name>
                           </manifestSection>
                        </manifestSections>
                     </archive>
                     <excludes>
                        <exclude>**/xsl/**</exclude>
                     </excludes>
                     <includes>
                        <include>com/**</include>
                        <include>META-INF/*.*</include>
                        <include>META-INF/maven/**</include>
                        <include>META-INF/versions/11/module-info.class</include>
                     </includes>
                  </configuration>
               </execution>
            </executions>
         </plugin>

         <!-- attach xsd files to maven artifact -->
         <plugin>
            <artifactId>maven-assembly-plugin</artifactId>
            <version>${version.assembly.plugin}</version>
            <executions>
               <execution>
                  <goals>
                     <goal>single</goal>
                  </goals>
                  <phase>package</phase>
               </execution>
            </executions>
            <configuration>
               <descriptors>
                  <descriptor>assembly.xml</descriptor> 
                  <descriptor>xml-assembly.xml</descriptor> 
               </descriptors>
            </configuration>
         </plugin>
      </plugins>
   </build>

   <profiles>
      <profile>
         <id>java8-automodule</id>
         <activation>
            <jdk>[1.8,11)</jdk>
         </activation>
         <build>
            <plugins>
               <plugin>
                  <groupId>org.apache.maven.plugins</groupId>
                  <artifactId>maven-compiler-plugin</artifactId>
                  <executions>
                     <execution>
                        <id>default-compile</id>
                        <configuration>
                           <jdkToolchain>
                              <version>1.8</version>
                           </jdkToolchain>
                           <source>8</source>
                           <target>8</target>
                        </configuration>
                     </execution>
                  </executions>
               </plugin>
               <plugin>
                  <groupId>org.apache.maven.plugins</groupId>
                  <artifactId>maven-jar-plugin</artifactId>
                  <executions>
                     <execution>
                        <id>default-jar</id>
                        <configuration>
                           <archive>
                              <manifestEntries>
                                 <Automatic-Module-Name>${automatic.module.name}</Automatic-Module-Name>
                              </manifestEntries>
                           </archive>
                        </configuration>
                     </execution>
                  </executions>
               </plugin>
            </plugins>
         </build>
      </profile>

      <profile>
         <id>java11-module</id>
         <activation>
            <jdk>[11,)</jdk>
         </activation>
         <build>
            <plugins>
               <plugin>
                  <groupId>org.codehaus.mojo</groupId>
                  <artifactId>build-helper-maven-plugin</artifactId>
                  <executions>
                     <execution>
                        <id>add-module-info</id>
                        <phase>process-sources</phase>
                        <goals>
                           <goal>add-source</goal>
                        </goals>
                        <configuration>
                           <sources>
                              <source>${basedir}/src/main/module</source>
                           </sources>
                        </configuration>
                     </execution>
                  </executions>
               </plugin>
               <plugin>
                  <groupId>org.apache.maven.plugins</groupId>
                  <artifactId>maven-compiler-plugin</artifactId>
                  <executions>
                     <execution>
                        <id>default-compile</id>
                        <configuration>
                           <jdkToolchain>
                              <version>1.8</version>
                           </jdkToolchain>
                           <source>1.8</source>
                           <target>1.8</target>
                           <excludes>
                              <exclude>module-info.java</exclude>
                           </excludes>
                        </configuration>
                     </execution>
                     <execution>
                        <id>compile-module</id>
                        <goals>
                           <goal>compile</goal>
                        </goals>
                        <configuration>
                           <jdkToolchain>
                              <version>[11,)</version>
                           </jdkToolchain>
                           <source>11</source>
                           <target>11</target>
                           <!-- compile everything to ensure module-info contains right entries -->
                           <multiReleaseOutput>true</multiReleaseOutput>
                           <release>11</release>
                        </configuration>
                     </execution>
                  </executions>
               </plugin>
               <plugin>
                  <groupId>org.apache.maven.plugins</groupId>
                  <artifactId>maven-jar-plugin</artifactId>
                  <executions>
                     <execution>
                        <id>default-jar</id>
                        <configuration>
                           <archive>
                              <manifestEntries>
                                 <Multi-Release>true</Multi-Release>
                              </manifestEntries>
                           </archive>
                        </configuration>
                     </execution>
                  </executions>
               </plugin>
               <plugin>
                  <groupId>org.codehaus.mojo</groupId>
                  <artifactId>exec-maven-plugin</artifactId>
                  <executions>
                     <execution>
                        <id>setup-java-module-version</id>
                        <phase>package</phase>
                        <goals>
                           <goal>exec</goal>
                        </goals>
                        <configuration>
                           <executable>jar</executable>
                           <commandlineArgs>--verbose --update --file ${project.build.directory}/${project.artifactId}.jar --module-version ${GENESYS.PROJECT.VERSION} --release 11</commandlineArgs>
                           <workingDirectory>${basedir}</workingDirectory>
                        </configuration>
                     </execution>
                  </executions>
               </plugin>
            </plugins>
         </build>
      </profile>

      <profile>
         <id>osgi</id>
         <properties>
            <manifest.file>${project.basedir}/target/classes/META-INF/MANIFEST.MF</manifest.file>
         </properties>
         <build>
            <plugins>
               <plugin>
                  <groupId>org.apache.felix</groupId>
                  <artifactId>maven-bundle-plugin</artifactId>
                  <extensions>true</extensions>
                  <executions>
                     <execution>
                        <id>bundle-manifest</id>
                        <phase>process-classes</phase>
                        <goals>
                           <goal>manifest</goal>
                        </goals>
                     </execution>
                  </executions>
                  <configuration>
                     <instructions>
                        <Bundle-Name>Platform SDK Contacts Protocols</Bundle-Name>
                        <Private-Package>
                           com.genesyslab.platform.contacts.protocol.server,
                           com.genesyslab.platform.contacts.*runtime* 
                        </Private-Package>
                        <Export-Package>
                           com.genesyslab.platform.contacts.protocol;version=${project.version};-noimport:=true,
                           com.genesyslab.platform.contacts.protocol.contactserver;version=${project.version};-noimport:=true,
                           com.genesyslab.platform.contacts.protocol.contactserver.requests;version=${project.version};-noimport:=true,
                           com.genesyslab.platform.contacts.protocol.contactserver.events;version=${project.version};-noimport:=true 
                        </Export-Package>
                        <Import-Package>
                           com.genesyslab.platform.commons;version="[${@},${@}]",
                           com.genesyslab.platform.commons.*;version="[${@},${@}]",
                           com.genesyslab.platform.openmedia.protocol;version="[${@},${@}]",
                           com.genesyslab.platform.openmedia.protocol.*;version="[${@},${@}]",
                           * 
                        </Import-Package>
                     </instructions>
                  </configuration>
               </plugin>
            </plugins>
         </build>
      </profile>

      <profile>
         <id>add-src</id>
         <build>
            <plugins>
               <plugin>
                  <groupId>org.apache.maven.plugins</groupId>
                  <artifactId>maven-source-plugin</artifactId>
                  <version>2.1.2</version>
                  <executions>
                     <execution>
                        <id>attach-sources</id>
                        <goals>
                           <goal>jar-no-fork</goal>
                        </goals>
                     </execution>
                  </executions>
               </plugin>
            </plugins>
         </build>
      </profile>
      <profile>
         <id>doc</id>
         <build>
            <plugins>
               <!-- read doc.properties file, prepared by doc team -->
               <plugin>
                  <groupId>org.codehaus.mojo</groupId>
                  <artifactId>properties-maven-plugin</artifactId>
                  <version>${version.properties.plugin}</version>
                  <executions>
                     <execution>
                        <goals>
                           <goal>read-project-properties</goal>
                        </goals>
                        <configuration>
                           <files>
                              <file>${doc.dir}/doc.properties</file>
                           </files>
                        </configuration>
                        <phase>prepare-package</phase>
                     </execution>
                  </executions>
               </plugin>
               <!--copy psdk_doc content to project's javadoc directory -->
               <plugin>
                  <artifactId>maven-resources-plugin</artifactId>
                  <version>2.5</version>
                  <executions>
                     <execution>
                        <id>copy-resources</id>
                        <phase>process-resources</phase>
                        <goals>
                           <goal>copy-resources</goal>
                        </goals>
                        <configuration>
                           <outputDirectory>${basedir}/src/main/javadoc</outputDirectory>
                           <resources>
                              <resource>
                                 <directory>${doc.dir}/pre-build</directory>
                                 <filtering>false</filtering>
                              </resource>
                              <resource>
                                 <directory>${doc.dir}/post-build</directory>
                                 <filtering>false</filtering>
                              </resource>
                           </resources>
                        </configuration>
                     </execution>
                  </executions>
               </plugin>
               <plugin>
                  <groupId>org.apache.maven.plugins</groupId>
                  <artifactId>maven-javadoc-plugin</artifactId>
                  <executions>
                     <execution>
                        <goals>
                           <goal>jar</goal>
                        </goals>
                     </execution>
                  </executions>
                  <configuration>
                     <outputDirectory>${project.build.directory}/doc/api</outputDirectory>
                     <docfilessubdirs>true</docfilessubdirs>
                     <author>true</author>
                     <nodeprecated>false</nodeprecated>
                     <nodeprecatedlist>false</nodeprecatedlist>
                     <notree>false</notree>
                     <nonavbar>false</nonavbar>
                     <noindex>false</noindex>
                     <show>public</show>
                     <source>1.8</source>
                     <splitindex>true</splitindex>
                     <use>true</use>
                     <version>true</version>
                     <quiet>true</quiet>
                     <overview>${doc.dir}/pre-build/welcome.html</overview>
                     <sourcepath>
                        ${sdk_commons.dir}\SystemJava\logging\src\main\java;
                        ${sdk_commons.dir}\SystemJava\system\src\main\java;
                        ${sdk_commons.dir}\CommonsProtocolsConnectionJava\src\main\java;
                        ${sdk_commons.dir}\CommonsJava\src\main\java;
                        ${sdk_commons.dir}\KVListsJava\kvlists\src\main\java;
                        ${basedir}\src\main\java;
                        ${basedir}\src\generated 
                     </sourcepath>
                     <excludePackageNames> *.runtime; *.impl;
                        *.interceptor; *.timer; *.processing; *.xml;
                        *.collections.util; 
                     </excludePackageNames>
                     <doctitle>${text.doc.title}</doctitle>
                     <header>${text.doc.header}</header>
                     <footer>${text.doc.footer}</footer>
                     <bottom>${text.doc.bottom}</bottom>
                  </configuration>
               </plugin>
            </plugins>
         </build>
      </profile>
      <profile>
         <id>eclipse-custom-settings</id>
         <activation>
            <property>
               <name>eclipse.application</name>
            </property>
         </activation>
         <build>
            <pluginManagement>
               <plugins>
                  <!-- Suppress Eclipse error messages "Plugin execution 
                     not covered by lifecycle" -->
                  <plugin>
                     <groupId>org.eclipse.m2e</groupId>
                     <artifactId>lifecycle-mapping</artifactId>
                     <version>1.0.0</version>
                     <configuration>
                        <lifecycleMappingMetadata>
                           <pluginExecutions>
                              <pluginExecution>
                                 <pluginExecutionFilter>
                                    <groupId>org.codehaus.mojo</groupId>
                                    <artifactId>exec-maven-plugin</artifactId>
                                    <versionRange>[1.0,)</versionRange>
                                    <goals>
                                       <goal>exec</goal>
                                    </goals>
                                 </pluginExecutionFilter>
                                 <action>
                                    <ignore/>
                                 </action>
                              </pluginExecution>
                                <pluginExecution>
                                 <pluginExecutionFilter>
                                    <groupId>org.apache.maven.plugins</groupId>
                                    <artifactId>maven-antrun-plugin</artifactId>
                                    <versionRange>[1.0,)</versionRange>
                                    <goals>
                                       <goal>run</goal>
                                    </goals>
                                 </pluginExecutionFilter>
                                 <action>
                                    <ignore/>
                                 </action>
                              </pluginExecution>
                           </pluginExecutions>
                        </lifecycleMappingMetadata>
                     </configuration>
                  </plugin>
               </plugins>
            </pluginManagement>
         </build>
      </profile>
      <profile>
         <id>resultpath-copy</id>
         <build>
            <plugins>
               <plugin>
                  <artifactId>maven-antrun-plugin</artifactId>
                  <executions>
                     <execution>
                        <phase>install</phase>
                        <configuration>
                           <tasks>
                              <echo>genesys.resultpath:${genesys.resultpath}</echo>
                              <echo>project.build.directory:${project.build.directory}</echo>
                              <echo>settings.localRepository:${settings.localRepository}</echo>
                              <copy todir="${genesys.resultpath}/release">
                                 <fileset dir="${project.build.directory}">
                                    <include name="${project.artifactId}.jar"/>
                                 </fileset>
                              </copy>
                              <copy flatten="true" todir="${genesys.resultpath}/release/maven/${project.artifactId}">
                                 <fileset dir="${settings.localRepository}/com/genesyslab/platform/${project.artifactId}">
                                    <include name="**/${project.artifactId}-${project.version}.jar"/>
                                    <include name="**/${project.artifactId}-${project.version}-sources.jar"/>
                                    <include name="**/${project.artifactId}-${project.version}.pom"/>
                                    <include name="**/${project.artifactId}-${project.version}-xsd.zip"/>
                                    <include name="**/${project.artifactId}-${project.version}-xml.zip"/>
                                 </fileset>
                              </copy>
                              <copy file="${xml.dir}/ContactServerMessages.xsd" tofile="${genesys.resultpath}/release/ContactServerMessages.xsd"/>
                           </tasks>
                        </configuration>
                        <goals>
                           <goal>run</goal>
                        </goals>
                     </execution>
                  </executions>
               </plugin>
            </plugins>
         </build>
      </profile>
   </profiles>
   <reporting>
      <plugins>
         <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-project-info-reports-plugin</artifactId>
            <version>2.4</version>
         </plugin>
      </plugins>
   </reporting>
   <distributionManagement>
      <repository>
         <uniqueVersion>false</uniqueVersion>
         <id>${prod.distribution.repository.id}</id>
         <name>${prod.distribution.repository.name}</name>
         <url>${prod.distribution.repository.url}</url>
      </repository>
      <snapshotRepository>
         <uniqueVersion>false</uniqueVersion>
         <id>${distribution.repository.id}</id>
         <name>${distribution.repository.name}</name>
         <url>${distribution.repository.url}</url>
      </snapshotRepository>
   </distributionManagement>
</project>